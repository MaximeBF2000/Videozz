p: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  padding: 1rem $container_size;\r\n  background-color: rgba($darkgrey, .7);\r\n  // background-color: transparent;\r\n  display: flex;\r\n  justify-content: space-between;\r\n\r\n  &.solid{\r\n    background-color: #111;\r\n  }\r\n\r\n  .logo{\r\n    margin-left: 10px;\r\n  }\r\n\r\n  .toggle_darkmode{\r\n    all: unset;\r\n    font-size: .8rem;\r\n    padding: 0 1rem;\r\n    border-radius: .25rem;\r\n    background-color: $white;\r\n    color: #000;\r\n    cursor: pointer;\r\n  }\r\n}","src\\styles\\_config.scss":"// Vars\r\n\r\n$ff: \"Montserrat\", sans-serif;\r\n\r\n$bgc: #0d5ee0;\r\n$darkgrey: #262626;\r\n\r\n$white: #fefefe;\r\n\r\n$container_size: 3rem;\r\n\r\n\r\n\r\n// Classes\r\n.center{\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.center-h{\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n}","src\\styles\\_banner.scss":"@import \"./config\";\r\n\r\n.banner{\r\n  position: relative;\r\n  background-size: cover;\r\n  background-position: center;\r\n  object-fit: cover;\r\n  height: 100vh;\r\n  @extend .center-h;\r\n\r\n  .banner_content{\r\n    padding: 0 2rem;\r\n\r\n    .banner_title{\r\n      font-size: 3rem;\r\n    }\r\n\r\n    .banner_buttons{\r\n      margin-top: 1rem;\r\n      margin-bottom: 2.5rem;\r\n\r\n      .banner_btn{\r\n        all: unset;\r\n        cursor: pointer;\r\n        padding: .3rem 2.5rem;\r\n        margin-right: 1rem;\r\n        border-radius: .25rem;\r\n        background-color: rgba($darkgrey, .9);\r\n        transition: .3s;\r\n\r\n        &:hover{\r\n          background-color: rgba($white, .9);\r\n          color: #000;\r\n        }\r\n      }\r\n\r\n    }\r\n\r\n    .banner_description{\r\n      font-size: .9rem;\r\n      max-width: 500px;\r\n      line-height: 1.4;\r\n    }\r\n  }\r\n\r\n}\r\n","src\\styles\\_films.scss":"@import \"./config\";\r\n\r\n$filmItemWidth: 150px;\r\n\r\n.filmCategory{\r\n  margin: 2rem 0;\r\n  padding: 0 $container_size;\r\n  position: relative;\r\n\r\n  .categoryTitle{\r\n    margin-left: 10px;\r\n  }\r\n\r\n  .chevron{\r\n    color: $white;\r\n    opacity: .3;\r\n    font-size: 3rem;\r\n    position: absolute;\r\n    top: 50%;\r\n    transform: translateY(-50%);\r\n    z-index: 1;\r\n    cursor: pointer;\r\n    background: transparent;\r\n\r\n    $chevron_position: 0px;\r\n\r\n    svg path{\r\n      pointer-events: none;\r\n    }\r\n\r\n    &:hover{\r\n      opacity: 1;\r\n    }\r\n\r\n    &.chevronLeft{\r\n      left: $chevron_position;\r\n    }\r\n    &.chevronRight{\r\n      right: $chevron_position;\r\n    }\r\n  }\r\n\r\n  .filmRow{\r\n    display: flex;\r\n    overflow-x: scroll;\r\n    padding: 1rem 0;\r\n    transition: .3s;\r\n\r\n    &::-webkit-scrollbar{\r\n      display: none;\r\n    }\r\n\r\n    .filmItem{\r\n      width: $filmItemWidth;\r\n      background-color: transparent /* OU QUAND ON MET LE TITRE : $darkgrey */;\r\n      margin: 0 10px;\r\n      cursor: pointer;\r\n\r\n      &:hover{\r\n        transform: scale(1.1);\r\n      }\r\n\r\n      .filmImg{\r\n        width: $filmItemWidth;\r\n\r\n        img{\r\n          width: 100%;\r\n          object-fit: contain;\r\n        }\r\n      }\r\n\r\n      .filmTitleContainer{\r\n        @extend .center;\r\n        width: 100%;\r\n        height: 2rem;\r\n        padding: 0 .3rem;\r\n        padding-bottom: .5rem;\r\n        \r\n        .filmTitle{\r\n          width: 100%;\r\n          text-align: center;\r\n          overflow: hidden;\r\n          white-space: nowrap;\r\n        }\r\n      }\r\n\r\n    }\r\n\r\n    &.bigger{\r\n      .filmItem{\r\n        width: $filmItemWidth + 65;\r\n\r\n        .filmImg{\r\n          width: $filmItemWidth + 65;\r\n        }\r\n      }\r\n    }\r\n\r\n  }\r\n\r\n}"},"lineCount":null}},"error":null,"hash":"3aa0ffec514cc376db14979fcdfdb133","cacheData":{"env":{}}}